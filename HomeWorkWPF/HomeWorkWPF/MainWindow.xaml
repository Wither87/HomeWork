<Window x:Class="HomeWorkWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HomeWorkWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="450">
    <Canvas>
        <Button Canvas.Left="10" Canvas.Top="10">(10,10)</Button>
        <Button Canvas.Left="100" Canvas.Top="50"
                Width="100" Height="70" Panel.ZIndex="1">(100,50)</Button>
        <Button Canvas.Left="70" Canvas.Top="100"
                Width="70" Height="30">(70,100)</Button>
    </Canvas>
</Window>
